# http://editorconfig.org

root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true

[*.kt]
indent_style = space
indent_size = 4
trim_trailing_whitespace = true

[*.xml]
indent_style = space
trim_trailing_whitespace = true

[*.gradle]
indent_style = space
trim_trailing_whitespace = true

[*.md]
indent_style = space
indent_size = 4
trim_trailing_whitespace = true
